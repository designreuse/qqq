/* 与bootstrap的.panel及.panel-default一致，防止bootstrap的样式被easyui重写 --END-- */

/* 左树的最小宽度 */
.min-width-200 {
	min-width: 200px;
}

/* 标签页内的内容页面的最小宽度(检索栏，容纳3个控件的最小宽度) */
.min-width-880 {
	min-width: 880px;
}

.min-width-980 {
	min-width: 980px;
}

/* datagrid的最小高度-中尺寸 */
.min-height-m {
	min-height: 410px;
}

/* datagrid的最小高度-小尺寸 */
.min-height-s {
	min-height: 210px;
}

/* 设置easyui的tabs panel的position，用来配合.navbar-absolute，将导航条固定在tab内容的顶部 */
#MainContentTab>.tabs-panels>.panel.panel-htop {
	position: relative;
}

/* bootstarp的.container和.container-fluid的修饰类，用来去掉padding */
.container-no-padding {
	padding: 0;
}

/* 覆盖bootstrap的.navbar-fixed-top，将position改为absolute，作为.navbar-fixed-top的修饰类，要配合.navbar-fixed-top一起使用 */
.navbar-absolute {
	position: absolute;
	background-color: white;
}

/* 覆盖bootstrap的.navbar-brand，用来将新建/修改画面的顶部"标题-按钮栏"中的标题padding去掉 */
#MainContentTab .navbar-absolute .container-fix-width-m .navbar-brand {
	padding-left: 0;
}

/* bootstarp导航条，右侧的按钮组样式 --Begin-- */
.navbar-right-button-group {
	margin-right: 0 !important;
}

.navbar-right-button-group button {
	margin-right: 10px;
}
/* bootstarp导航条，右侧的按钮组样式 --END-- */

/* 参考bootstrap的.container，设置固定宽度 */
.min-width-880 .container-fix-width-m {
	width: 860px;
	padding-right: 15px;
	padding-left: 15px;
	margin-right: auto;
	margin-left: auto;
}

/* wrapper of easyui tabs #MainContentTab, ref: .content from AdminLTE.css */
.main-content-tab-wrapper {
	/* .main-header的height:50px，.main-footer-thin的height:35px，合计85px */
	/* 将此wrapper的高度设定为剩余的px，从而限制内部的easyui-tabs的最大高度 */
	/* height: calc(100vh - 85px); */
	height: calc(100vh - 65px); /* 去掉了.main-footer-thin的内容 */
	margin-right: auto;
	margin-left: auto;
	padding: 5px
}

/* 结合bootstrap的easyui-datagrid包裹样式 */
.datagrid-wrapper {
	margin-left: -15px;
	margin-right: -15px;
	padding: 6px 10px;
}

/* bootstrap的单行单列样式，即.row中唯一的.col中唯一的cell，调整左右的边距 */
.row-single-col-cell {
	margin-left: -15px;
	margin-right: -15px;
	padding: 6px 10px;
}

/* 配合bootstrap的.row一起使用，增加行间距 --Begin-- */
.row-gapped-top-s {
	padding-top: 5px;
}

.row-gapped-top-m {
	padding-top: 10px;
}

.row-gapped-both-s {
	padding-top: 5px;
	padding-bottom: 5px;
}
/* 配合bootstrap的.row一起使用，增加行间距 --END-- */

/* 配合bootstrap的.col-*-*，减少列间距 */
.col-gapped-none {
	padding-left: 0px;
	padding-right: 0px;
}

.col-gapped-s {
	padding-left: 5px;
	padding-right: 5px;
}

.col-gapped-xs {
	padding-left: 2px;
	padding-right: 2px;
}

.col-gapped-left-m {
	padding-left: 10px;
	padding-right: 0px;
}

.col-gapped-left-l {
	padding-left: 20px;
	padding-right: 0px;
}

.col-gapped-right-xs {
	padding-left: 0px;
	padding-right: 2px;
}

/* 列中的单元格，设定高度与垂直居中 */
.col-cell-height-m {
	min-height: 32px;
	line-height: 32px;
}

.col-cell-checkbox {
	vertical-align: middle;
	margin: 0px !important;
}

