/* Customize the 3rd party css */

/**************** AdminLTE Begin ****************/

/* Copied from AdminLTE.css */
/* .menu is conflicted with easyui. Change class name from .menu to .menu-lte --Begin-- */
.navbar-nav>.notifications-menu>.dropdown-menu>li .menu-lte, .navbar-nav>.messages-menu>.dropdown-menu>li .menu-lte,
	.navbar-nav>.tasks-menu>.dropdown-menu>li .menu-lte {
	max-height: 200px;
	margin: 0;
	padding: 0;
	list-style: none;
	overflow-x: hidden;
}

.navbar-nav>.notifications-menu>.dropdown-menu>li .menu-lte>li>a,
	.navbar-nav>.messages-menu>.dropdown-menu>li .menu-lte>li>a,
	.navbar-nav>.tasks-menu>.dropdown-menu>li .menu-lte>li>a {
	display: block;
	white-space: nowrap;
	/* Prevent text from breaking */
	border-bottom: 1px solid #f4f4f4;
}

.navbar-nav>.notifications-menu>.dropdown-menu>li .menu-lte>li>a:hover,
	.navbar-nav>.messages-menu>.dropdown-menu>li .menu-lte>li>a:hover,
	.navbar-nav>.tasks-menu>.dropdown-menu>li .menu-lte>li>a:hover {
	background: #f4f4f4;
	text-decoration: none;
}

.navbar-nav>.notifications-menu>.dropdown-menu>li .menu-lte>li>a {
	color: #444444;
	overflow: hidden;
	text-overflow: ellipsis;
	padding: 10px;
}

.navbar-nav>.notifications-menu>.dropdown-menu>li .menu-lte>li>a>.glyphicon,
	.navbar-nav>.notifications-menu>.dropdown-menu>li .menu-lte>li>a>.fa,
	.navbar-nav>.notifications-menu>.dropdown-menu>li .menu-lte>li>a>.ion {
	width: 20px;
}

.navbar-nav>.messages-menu>.dropdown-menu>li .menu-lte>li>a {
	margin: 0;
	padding: 10px 10px;
}

.navbar-nav>.messages-menu>.dropdown-menu>li .menu-lte>li>a>div>img {
	margin: auto 10px auto auto;
	width: 40px;
	height: 40px;
}

.navbar-nav>.messages-menu>.dropdown-menu>li .menu-lte>li>a>h4 {
	padding: 0;
	margin: 0 0 0 45px;
	color: #444444;
	font-size: 15px;
	position: relative;
}

.navbar-nav>.messages-menu>.dropdown-menu>li .menu-lte>li>a>h4>small {
	color: #999999;
	font-size: 10px;
	position: absolute;
	top: 0;
	right: 0;
}

.navbar-nav>.messages-menu>.dropdown-menu>li .menu-lte>li>a>p {
	margin: 0 0 0 45px;
	font-size: 12px;
	color: #888888;
}

.navbar-nav>.messages-menu>.dropdown-menu>li .menu-lte>li>a:before,
	.navbar-nav>.messages-menu>.dropdown-menu>li .menu-lte>li>a:after {
	content: " ";
	display: table;
}

.navbar-nav>.messages-menu>.dropdown-menu>li .menu-lte>li>a:after {
	clear: both;
}

.navbar-nav>.tasks-menu>.dropdown-menu>li .menu-lte>li>a {
	padding: 10px;
}

.navbar-nav>.tasks-menu>.dropdown-menu>li .menu-lte>li>a>h3 {
	font-size: 14px;
	padding: 0;
	margin: 0 0 10px 0;
	color: #666666;
}

.navbar-nav>.tasks-menu>.dropdown-menu>li .menu-lte>li>a>.progress {
	padding: 0;
	margin: 0;
}
/* .menu-lte related --END-- */

/* 改变边框颜色，以便在白色背景上较明显的显示边框 */
.info-box.info-box-border {
	border: 1px solid #eee;
	border-radius: 5px;
}

/* 重写form-control的样式，模仿EasyUI --Begin-- */
.form-control.ez-like {
	border-radius: 4px 4px 4px 4px;
	width: 100%;
	height: 32px;
	padding: 4px 10px;
}

.form-control.ez-like[readonly] {
	background-color: #fafafa;
}

.input-group>.form-control.ez-like[readonly] {
	/* .input-group下的.form-control.ez-like默认就是readonly */
	background-color: #fff;
}

.input-group.readonly>.form-control.ez-like[readonly] {
	/* 只有当.input-group也是.readonly，此时其下的.form-control.ez-like才设置背景色 */
	background-color: #fafafa;
}

.form-control.ez-like[disabled] {
	background-color: #eee;
}

select.form-control.ez-like {
	padding: 4px 7px;
}

.form-control.ez-like:focus {
	border-color: #c4c4c4;
	-moz-box-shadow: 0 0 3px 0 #ddd;
	-webkit-box-shadow: 0 0 3px 0 #ddd;
	box-shadow: 0 0 3px 0 #ddd;
}
/* 重写form-control的样式，模仿EasyUI --End-- */

/*增加左侧菜单项第二级子菜单的缩进*/
.sidebar-menu>.treeview>ul.treeview-menu {
	padding-left: 10px;
}

/**************** AdminLTE --End-- ****************/

/**************** EasyUI --Begin-- ****************/

/* Everything is material theme but dialog is gray theme */
/* Below are dialog styles copied from gray theme. --Begin-- */
.window, .window-shadow {
	position: absolute;
	-moz-border-radius: 5px 5px 5px 5px;
	-webkit-border-radius: 5px 5px 5px 5px;
	border-radius: 5px 5px 5px 5px;
}

.window-shadow {
	background: #ccc;
	-moz-box-shadow: 2px 2px 3px #cccccc;
	-webkit-box-shadow: 2px 2px 3px #cccccc;
	box-shadow: 2px 2px 3px #cccccc;
	filter: progid:DXImageTransform.Microsoft.Blur(pixelRadius=2, MakeShadow=false,
		ShadowOpacity=0.2);
}

.window, .window .window-body {
	border-color: #D3D3D3;
}

.window {
	background-color: #f3f3f3;
	background: -webkit-linear-gradient(top, #F8F8F8 0, #eeeeee 20%);
	background: -moz-linear-gradient(top, #F8F8F8 0, #eeeeee 20%);
	background: -o-linear-gradient(top, #F8F8F8 0, #eeeeee 20%);
	background: linear-gradient(to bottom, #F8F8F8 0, #eeeeee 20%);
	background-repeat: repeat-x;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#F8F8F8,
		endColorstr=#eeeeee, GradientType=0);
}

.window-proxy {
	border: 1px dashed #D3D3D3;
}

.window-proxy-mask, .window-mask {
	background: #ccc;
}

.window .panel-footer {
	border: 1px solid #D3D3D3;
	position: relative;
	top: -1px;
}

.dialog-tool-separator {
	float: left;
	height: 24px;
	border-left: 1px solid #ccc;
	border-right: 1px solid #fff;
	margin: 2px 1px;
}

.dialog-toolbar {
	border-color: #D3D3D3 #D3D3D3 #ddd #D3D3D3;
}

.dialog-button {
	border-color: #ddd #D3D3D3 #D3D3D3 #D3D3D3;
}
/* dialog from gray theme --END-- */

/* Change the background-color and border of .tabs-header to match the content of AdminLTE --Begin-- */
.tabs-header {
	border-width: 0px;
}

.tabs-header, .tabs-tool {
	background-color: #ecf0f5;
}
/* .tabs-header --END-- */

/*增加panel头部的icon与title的距离*/
.panel-with-icon {
	padding-left: 23px;
}

/*增加window头部的icon与title的距离*/
.window .window-header .panel-with-icon {
	padding-left: 23px;
}

/* 弹出window时的淡色的背景遮挡 */
.window-proxy-mask, .window-mask {
	background: #aaa;
}

/* 按钮的文本不换行 */
.l-btn-text {
	white-space: nowrap;
	word-wrap: normal;
}

/* 增加输入框的padding */
.textbox .textbox-text {
	padding: 4px 10px;
}

/*增加只读的textbox的背景区分*/
.textbox-readonly .textbox-text {
	background-color: #EBEBE4;
}

/* 增加combobox内item的最小高度 */
.combobox-item {
	min-height: 15px;
}

/*layout外层panel背景设为透明*/
.layout-panel {
	background: transparent;
}

/* datagrid中当文字超出cell宽度时，显示'...' */
.datagrid-cell {
	text-overflow: ellipsis;
}

/* 重写easyui的messager样式，影响气泡弹出框 */
.messager-body.panel-body-noheader {
	font-weight: bold;
	color: #575765;
	height: 16px;
	line-height: 16px;
	border: 0px;
	padding: 0px;
	text-align: center;
	background: transparent;
}

/*bug - IE下会导致tooltip的箭头无法正常显示*/
.tooltip {
	filter: none;
}

/*针对material theme，调深datagrid的标题栏的文字 */
.datagrid-header .datagrid-cell, .datagrid-header .datagrid-cell-group {
	color: #333;
}

/* 增加dialog内容的padding */
.easyui-dialog.panel-body.window-body {
	padding: 5px 10px 15px 10px;
}

/* 设置弹出画面底部按钮栏，增加右侧padding */
.dialog-button {
	padding-right: 20px;
}

/* 设置弹出画面底部按钮栏中，按钮的最小宽度 */
.dialog-button a.l-btn {
	min-width: 65px;
}

/* tabs的工具栏，去掉边框 */
.tabs-tool-no-border {
	border-top-width: 0px !important;
	border-left-width: 0px !important;
	border-right-width: 0px !important;
}

/*layout的.panel会被bootstrap影响，重写受影响的属性*/
.panel.layout-panel {
	webkit-box-shadow: none;
	box-shadow: none;
}

/**************** EasyUI --End-- ****************/

/**************** Customized --Begin-- ****************/

/* 加载easyui画面时的遮盖，防止画面跳动 */
.jeasyui-loading {
	position: absolute;
	z-index: 99999;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 100%;
	background: #f4f4f4 url(../img/loading.gif) no-repeat 15px 15px;
}

/* AJAX时，弹出'loading...'等待提示 */
.loading-prompt {
	background: #fff url(../img/loading.gif) no-repeat 8px 11px !important;
	text-align: center;
}

/* 文本不换行，超出显示省略号 */
.text-nowrap-ellipsis {
	white-space: nowrap;
	word-wrap: normal;
	overflow: hidden;
	text-overflow: ellipsis;
}

/* 检索栏相关样式 --Begin-- */
.filter-wrapper {
	margin-left: -15px;
	margin-right: -15px;
	padding: 6px 10px;
	overflow: visible;
}

.filter-wrapper .filter-form-control-row {
	height: 32px;
}

.filter-wrapper .filter-form-control-row .filter-form-group {
	margin-right: 10px;
	float: left !important;
}

.filter-control-label {
	margin-right: 5px;
	vertical-align: middle;
}

.filter-tag-row { /** 占位用row */
	margin-top: 8px;
	height: 20px;
}

.filter-tag-row .filter-tag-box {
	/** 配合.filter-tag-box-hover改变z-index从而浮动的容器 */
	float: left;
	position: relative;
	height: 20px;
	width: 100%;
}

.filter-tag-row .filter-tag-box-hover {
	z-index: 1;
}

.filter-tag-row .filter-tag-inner-box {
	/** 配合.filter-tag-box-hover改变height的容器 */
	position: absolute;
	top: 0;
	left: 0;
	padding-right: 60px;
	height: 20px; /** hover时变为auto */
	width: 95%;
	overflow: hidden; /** 必须！配合height */
}

.filter-tag-row .filter-tag-box-hover .filter-tag-inner-box {
	/** 边框&阴影&背景色 */
	top: -1px;
	left: -1px;
	height: auto; /** hover时自动高度 */
	border: solid 1px #ccc;
	-webkit-box-shadow: 0 3px 3px rgba(0, 0, 0, .1);
	-moz-box-shadow: 0 3px 3px rgba(0, 0, 0, .1);
	box-shadow: 0 3px 3px rgba(0, 0, 0, .1);
	background: #F7FAFD;
}

.filter-tag-row .filter-tags { /** filter-tag的wrapper */
	width: 100%;
	float: left;
}

.filter-tag-row .filter-tag {
	float: left;
	display: inline;
	height: 20px;
	line-height: 20px;
	margin-right: 10px;
	margin-bottom: 5px;
	margin-left: 5px;
	cursor: pointer;
	background: #A0C4E4;
	color: #ffffff;
	white-space: nowrap;
	text-decoration: none;
}

.filter-tag-row .filter-tag-box-hover .filter-tag, .filter-tag-row .filter-tag-box-hover .filter-tag-pin
	{
	margin-top: 5px;
}

.filter-tag-row .filter-tag:hover {
	background: #7EB4E4;
}

.filter-tag-row .filter-tag .filter-tag-text {
	_display: inline;
	float: left;
	margin-left: 4px
}

.filter-tag-row .filter-tag .filter-tag-close {
	float: left;
	background: url("../img/icon.png") -59px -198px;
	width: 10px;
	height: 10px;
	display: inline-block;
	margin: 4px 5px 5px 10px;
}

.filter-tag-row .filter-tag-pin-wrapper {
	position: absolute;
	right: 0;
	top: 0px;
	padding-right: 10px;
}

.filter-tag-row .filter-tag-pin {
	text-decoration: none;
	display: inline-block;
	background: transparent;
	padding: 1px;
	border-width: 0;
	width: 18px;
	height: 18px;
}

.filter-tag-row .filter-tag-pin:hover, .filter-tag-row .filter-tag-pin-wrapper .filter-tag-pin-selected
	{
	background: #eaf2ff;
	padding: 0;
	border: 1px solid #b7d2ff;
	-moz-border-radius: 5px 5px 5px 5px;
	-webkit-border-radius: 5px 5px 5px 5px;
	border-radius: 5px 5px 5px 5px;
}

.filter-tag-row .filter-tag-pin .filter-tag-pin-icon {
	display: inline-block;
	width: 16px;
	height: 16px;
	background: url('../img/pin.png') no-repeat center center;
}

/* 检索栏相关样式 --END-- */

/* 减少AdminLTE的.main-footer的上下padding */
.main-footer-thin {
	padding-bottom: 7px;
	padding-top: 7px;
}

/* 菜单组的选中效果，左侧显示蓝条，ref: .skin-blue .sidebar-menu > li.active > a from skin-blue.css */
