	float: left;
	margin-left: 5px;
}

.dlg-checkbox span {
	margin-left: 5px;
	display: inline-block;
	line-height: 28px;
}

.dlg-checkbox input {
	margin-top: 8px;
}
/* 弹出画面底部的复制checkbox --END-- */

