package project.edge.domain.view;

import garage.origin.domain.view.ViewBean;

/**
 * @author angel_000
 *         保存项目角色的表现层DTO。
 */
public class ProjectRoleBean implements ViewBean {

    private String id;

    private String projectRoleName;
    private String shortName;
    private String projectRoleCode;
    private String pid_;
    private String pidText;
    private int level;
    private int projectRoleOrder;

    @Override
    public String getId() {
        // TODO Auto-generated method stub
        return this.id;
    }


    public String getProjectRoleName() {
        return this.projectRoleName;
    }


    public void setProjectRoleName(String projectRoleName) {
        this.projectRoleName = projectRoleName;
    }


    public String getShortName() {
        return this.shortName;
    }


    public void setShortName(String shortName) {
        this.shortName = shortName;
    }


    public String getProjectRoleCode() {
        return this.projectRoleCode;
    }


    public void setProjectRoleCode(String projectRoleCode) {
        this.projectRoleCode = projectRoleCode;
    }

    public String getPid_() {
        return this.pid_;
    }

    public void setPid_(String pid_) {
        this.pid_ = pid_;
    }

    public String getPidText() {
        return this.pidText;
    }

    public void setPidText(String pidText) {
        this.pidText = pidText;
    }


    public int getLevel() {
        return this.level;
    }


    public void setLevel(int level) {
        this.level = level;
    }


    public int getProjectRoleOrder() {
        return this.projectRoleOrder;
    }


    public void setProjectRoleOrder(int projectRoleOrder) {
        this.projectRoleOrder = projectRoleOrder;
    }


    public void setId(String id) {
        this.id = id;
    }


}
